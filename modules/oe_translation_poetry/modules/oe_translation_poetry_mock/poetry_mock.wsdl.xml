<?xml version="1.0" encoding="UTF-8"?>
<definitions name="PoetryTestServer" targetNamespace="http://localhost:8080/build/poetry-mock/server" xmlns:tns="http://localhost:8080/build/poetry-mock/server" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:ns="http://localhost:8080/build/poetry-mock/server/types">
  <types>
    <xsd:schema targetNamespace="http://localhost:8080/build/poetry-mock/server/types" xmlns="http://localhost:8080/build/poetry-mock/server/types"/>
  </types>
  <message name="requestServiceRequest">
    <part name="user" type="xsd:string"/>
    <part name="password" type="xsd:string"/>
    <part name="msg" type="xsd:string"/>
  </message>
  <message name="requestServiceResponse">
    <part name="requestServiceReturn" type="xsd:string">
    </part>
  </message>
  <portType name="PoetryTestServerPortType">
    <operation name="requestService">
      <input message="tns:requestServiceRequest"/>
      <output message="tns:requestServiceResponse"/>
    </operation>
  </portType>
  <binding name="PoetryTestServerBinding" type="tns:PoetryTestServerPortType">
    <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
    <operation name="requestService">
      <soap:operation soapAction="http://localhost:8080/build/poetry-mock/server/#requestService"/>
      <input>
      <soap:body use="literal" namespace="http://localhost:8080/build/poetry-mock/server"/>
      </input>
      <output>
        <soap:body use="literal" namespace="http://localhost:8080/build/poetry-mock/server"/>
      </output>
    </operation>
  </binding>
  <service name="PoetryTestServerService">
    <port name="PoetryTestServerPort" binding="tns:PoetryTestServerBinding">
      <soap:address location="http://localhost:8080/build/poetry-mock/server"/>
    </port>
  </service>
</definitions>
